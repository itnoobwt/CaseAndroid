[32m+[m[32m    @Override[m
[32m+[m[32m    public boolean onTouchEvent(MotionEvent event)[m
[32m+[m[32m    {[m
[32m+[m[32m        float x = event.getX();[m
[32m+[m[32m        float y = event.getY();[m
[32m+[m[32m        switch (event.getAction()){[m
[32m+[m[32m            case MotionEvent.ACTION_DOWN:[m
[32m+[m[32m                path.moveTo(x,y);[m
[32m+[m[32m                preX = x;[m
[32m+[m[32m                preY = y;[m
[32m+[m[32m                break;[m
[32m+[m[32m            case MotionEvent.ACTION_MOVE:[m
[32m+[m[32m                path.quadTo(preX,preY,x,y);[m
[32m+[m[32m                preX = x;[m
[32m+[m[32m                preY = y;[m
[32m+[m[32m                break;[m
[32m+[m[32m            case MotionEvent.ACTION_UP:[m
[32m+[m[32m                //è®²cacheBitmapç»˜åˆ¶åˆ°è¯¥Viewç»„ä»¶ä¸Š[m
[32m+[m[32m                cacheCanvas.drawPath(path,paint);[m
[32m+[m[32m                path.reset();  //æ¸…é™¤æ‰pathé‡Œçš„çº¿æ¡å’Œæ›²çº¿[m
[32m+[m[32m                break;[m
[32m+[m[32m        }[m
[32m+[m[32m        invalidate();[m
[32m+[m[32m        return true;[m
[32m+[m[32m    }[m
