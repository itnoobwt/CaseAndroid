[32m+[m[32mpackage com.system.caseandroid.six.widegt;[m
[32m+[m
[32m+[m[32mimport android.content.Context;[m
[32m+[m[32mimport android.content.res.TypedArray;[m
[32m+[m[32mimport android.graphics.Canvas;[m
[32m+[m[32mimport android.os.Handler;[m
[32m+[m[32mimport android.os.Message;[m
[32m+[m[32mimport android.util.AttributeSet;[m
[32m+[m[32mimport android.widget.ImageView;[m
[32m+[m[32mimport com.system.caseandroid.R;[m
[32m+[m
[32m+[m[32mimport java.util.Timer;[m
[32m+[m[32mimport java.util.TimerTask;[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Created by user on 2016/10/12.[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mpublic class AlphaImageView extends ImageView[m
[32m+[m[32m{[m
[32m+[m[32m    private int alphaDelta = 0;[m
[32m+[m[32m    private int curAlpha = 0;[m
[32m+[m[32m    private final int SPEED = 300;[m
[32m+[m[32m    private int duration=0;[m
[32m+[m[32m//    public AlphaImageView(Context context)[m
[32m+[m[32m//    {[m
[32m+[m[32m//        super(context);[m
[32m+[m[32m//    }[m
[32m+[m
[32m+[m[32m    public AlphaImageView(Context context, AttributeSet attrs)[m
[32m+[m[32m    {[m
[32m+[m[32m        super(context, attrs);[m
[32m+[m[32m        TypedArray typedArray = context.obtainStyledAttributes(attrs, R.styleable.AlphaImageView);[m
[32m+[m[32m        duration = typedArray.getInt(R.styleable.AlphaImageView_duration,0);[m
[32m+[m[32m        alphaDelta = 255*SPEED/duration;[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m
[32m+[m[32m    Handler handler = new Handler(){[m
[32m+[m[32m        @Override[m
[32m+[m[32m        public void handleMessage(Message msg)[m
[32m+[m[32m        {[m
[32m+[m[32m            super.handleMessage(msg);[m
[32m+[m[32m            if(msg.what == 0x123){[m
[32m+[m[32m                curAlpha += alphaDelta;[m
[32m+[m[32m                if(curAlpha > 600) curAlpha = 600;[m
[32m+[m[32m                AlphaImageView.this.setImageAlpha(curAlpha);[m
[32m+[m[32m            }[m
[32m+[m[32m        }[m
[32m+[m[32m    };[m
[32m+[m
[32m+[m[32m    @Override[m
[32m+[m[32m    protected void onDraw(Canvas canvas)[m
[32m+[m[32m    {[m
[32m+[m[32m        AlphaImageView.this.setImageAlpha(curAlpha);[m
[32m+[m[32m        super.onDraw(canvas);[m
[32m+[m[32m        final Timer timer = new Timer();[m
[32m+[m[32m        timer.schedule(new TimerTask()[m
[32m+[m[32m        {[m
[32m+[m
[32m+[m[32m            @Override[m
[32m+[m[32m            public void run()[m
[32m+[m[32m            {[m
[32m+[m[32m                Message msg = new Message();[m
[32m+[m[32m                msg.what = 0x123;[m
[32m+[m[32m                if(curAlpha>=255){[m
[32m+[m[32m                    timer.cancel();[m
[32m+[m[32m                }[m
[32m+[m[32m                else[m
[32m+[m[32m                    handler.sendMessage(msg);[m
[32m+[m[32m            }[m
[32m+[m[32m        },0,SPEED);[m
[32m+[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m
[32m+[m
[32m+[m
[32m+[m[32m}[m
