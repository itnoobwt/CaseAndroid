[32m+[m[32m    private long exitTime = 0;[m
[32m+[m
[32m+[m[32m    @Override[m
[32m+[m[32m    public boolean onKeyDown(int keyCode, KeyEvent event) {[m
[32m+[m[32m        if(keyCode == android.R.id.home){[m
[32m+[m[32m            if((System.currentTimeMillis()-exitTime) > 2000){[m
[32m+[m[32m                Toast.makeText(getApplicationContext(), "å†æŒ‰ä¸€æ¬¡é€€å‡ºç¨‹åº", Toast.LENGTH_SHORT).show();[m
[32m+[m[32m                exitTime = System.currentTimeMillis();[m
[32m+[m[32m            } else {[m
[32m+[m[32m                finish();[m
[32m+[m[32m                System.exit(0);[m
[32m+[m[32m            }[m
[32m+[m[32m            return true;[m
[32m+[m[32m        }[m
[32m+[m[32m        else if(keyCode == KeyEvent.KEYCODE_BACK && event.getAction() == KeyEvent.ACTION_DOWN){[m
[32m+[m[32m            if((System.currentTimeMillis()-exitTime) > 2000){[m
[32m+[m[32m                Toast.makeText(getApplicationContext(), "å†æŒ‰ä¸€æ¬¡é€€å‡ºç¨‹åº", Toast.LENGTH_SHORT).show();[m
[32m+[m[32m                exitTime = System.currentTimeMillis();[m
[32m+[m[32m            } else {[m
[32m+[m[32m                finish();[m
[32m+[m[32m                System.exit(0);[m
[32m+[m[32m            }[m
[32m+[m[32m            return true;[m
[32m+[m[32m        }[m
[32m+[m[32m        return super.onKeyDown(keyCode, event);[m
[32m+[m[32m    }[m
