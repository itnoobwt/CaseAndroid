[32m+[m[32mpackage com.system.caseandroid.six;[m
[32m+[m
[32m+[m[32mimport android.graphics.drawable.ClipDrawable;[m
[32m+[m[32mimport android.os.Handler;[m
[32m+[m[32mimport android.os.Message;[m
[32m+[m[32mimport android.support.v7.app.AppCompatActivity;[m
[32m+[m[32mimport android.os.Bundle;[m
[32m+[m[32mimport android.util.Log;[m
[32m+[m[32mimport android.widget.ImageView;[m
[32m+[m[32mimport com.system.caseandroid.R;[m
[32m+[m
[32m+[m[32mimport java.util.Timer;[m
[32m+[m[32mimport java.util.TimerTask;[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * 讲解Resoures资源使用[m
[32m+[m[32m * StateListDrawable 资源使用[m
[32m+[m[32m * LayerDrawable 资源使用[m
[32m+[m[32m * ShapeDrawable 资源使用[m
[32m+[m[32m * ClipDrawable  对象使用[m
[32m+[m[32m */[m
[32m+[m[32mpublic class ResouresActivity extends AppCompatActivity[m
[32m+[m[32m{[m
[32m+[m[32m    private ImageView imageView;[m
[32m+[m[32m    @Override[m
[32m+[m[32m    protected void onCreate(Bundle savedInstanceState)[m
[32m+[m[32m    {[m
[32m+[m[32m        super.onCreate(savedInstanceState);[m
[32m+[m[32m        setContentView(R.layout.activity_resoures);[m
[32m+[m
[32m+[m
[32m+[m
[32m+[m
[32m+[m[32m        imageView = (ImageView) findViewById(R.id.image);[m
[32m+[m[32m        final ClipDrawable drawable = (ClipDrawable)imageView.getDrawable();[m
[32m+[m[32m        Log.e("level",drawable.getLevel()+"       =");[m
[32m+[m[32m        final Handler handler = new Handler(){[m
[32m+[m[32m            @Override[m
[32m+[m[32m            public void handleMessage(Message msg)[m
[32m+[m[32m            {[m
[32m+[m[32m                super.handleMessage(msg);[m
[32m+[m[32m                if(msg.what == 0x12333)[m
[32m+[m[32m                    drawable.setLevel(drawable.getLevel() + 200);[m
[32m+[m[32m            }[m
[32m+[m[32m        };[m
[32m+[m
[32m+[m[32m        final Timer timer = new Timer();[m
[32m+[m[32m        timer.schedule(new TimerTask()[m
[32m+[m[32m        {[m
[32m+[m[32m            @Override[m
[32m+[m[32m            public void run()[m
[32m+[m[32m            {[m
[32m+[m[32m                try[m
[32m+[m[32m                {[m
[32m+[m[32m                    Message msg = new Message();[m
[32m+[m[32m                    msg.what = 0x12333;[m
[32m+[m[32m                    handler.sendMessage(msg);[m
[32m+[m
[32m+[m[32m                    if(drawable.getLevel() >=10000)[m
[32m+[m[32m                        timer.cancel();[m
[32m+[m[32m                }catch (Exception e){[m
[32m+[m[32m                    Log.e("error",e.getMessage());[m
[32m+[m[32m                }[m
[32m+[m[32m            }[m
[32m+[m[32m        },0,300);[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m}[m
