[32m+[m[32mpackage com.system.caseandroid.Fives;[m
[32m+[m
[32m+[m[32mimport android.app.Activity;[m
[32m+[m[32mimport android.content.ComponentName;[m
[32m+[m[32mimport android.content.CursorLoader;[m
[32m+[m[32mimport android.content.Intent;[m
[32m+[m[32mimport android.database.Cursor;[m
[32m+[m[32mimport android.net.Uri;[m
[32m+[m[32mimport android.provider.ContactsContract;[m
[32m+[m[32mimport android.support.v7.app.AppCompatActivity;[m
[32m+[m[32mimport android.os.Bundle;[m
[32m+[m[32mimport android.util.Log;[m
[32m+[m[32mimport android.view.View;[m
[32m+[m[32mimport android.widget.Button;[m
[32m+[m[32mimport android.widget.EditText;[m
[32m+[m[32mimport android.widget.Toast;[m
[32m+[m[32mimport com.system.caseandroid.R;[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * 讲解Intent用法、action、data、typ等等......[m
[32m+[m[32m */[m
[32m+[m[32mpublic class SecondActivity extends AppCompatActivity[m
[32m+[m[32m{[m
[32m+[m[32m    private Button btn;[m
[32m+[m[32m    private final int PICK_CONTACT = 0;[m
[32m+[m
[32m+[m[32m    @Override[m
[32m+[m[32m    protected void onCreate(Bundle savedInstanceState)[m
[32m+[m[32m    {[m
[32m+[m[32m        super.onCreate(savedInstanceState);[m
[32m+[m[32m        setContentView(R.layout.activity_second);[m
[32m+[m[32m        //        ComponentName componentName = getIntent().getComponent();[m
[32m+[m[32m        //        Toast.makeText(this, componentName.getPackageName()+"    "+componentName.getClassName(),[m
[32m+[m[32m        //                Toast.LENGTH_SHORT).show();[m
[32m+[m[32m        btn = (Button) findViewById(R.id.bn);[m
[32m+[m[32m        btn.setOnClickListener(new View.OnClickListener()[m
[32m+[m[32m        {[m
[32m+[m[32m            @Override[m
[32m+[m[32m            public void onClick(View v)[m
[32m+[m[32m            {[m
[32m+[m[32m                Intent intent = new Intent();[m
[32m+[m[32m                intent.setAction(Intent.ACTION_GET_CONTENT);[m
[32m+[m[32m                intent.setType("vnd.android.cursor.item/phone");[m
[32m+[m[32m                startActivityForResult(intent, PICK_CONTACT);[m
[32m+[m[32m            }[m
[32m+[m[32m        });[m
[32m+[m[32m    }[m
[32m+[m[32m    @Override[m
[32m+[m[32m    protected void onActivityResult(int requestCode, int resultCode, Intent data)[m
[32m+[m[32m    {[m
[32m+[m[32m        super.onActivityResult(requestCode, resultCode, data);[m
[32m+[m[32m        switch (requestCode)[m
[32m+[m[32m        {[m
[32m+[m[32m            case (PICK_CONTACT):[m
[32m+[m[32m                if (resultCode == Activity.RESULT_OK)[m
[32m+[m[32m                {[m
[32m+[m[32m                    Uri contactData = data.getData();[m
[32m+[m[32m                    CursorLoader cursorLoader = new CursorLoader(this, contactData, null, null, null, null);[m
[32m+[m[32m                    Cursor cursor = cursorLoader.loadInBackground();[m
[32m+[m[32m                    if (cursor.moveToFirst())[m
[32m+[m[32m                    {[m
[32m+[m[32m                        String contactId = cursor.getString(cursor.getColumnIndex(ContactsContract.Contacts._ID));[m
[32m+[m[32m                        //获取联系人的名字[m
[32m+[m[32m                        String name = cursor.getString(cursor.getColumnIndex(ContactsContract.Contacts.DISPLAY_NAME));[m
[32m+[m[32m                        String phoneNumber = "次联系人暂未输入电话号码";[m
[32m+[m[32m                        try[m
[32m+[m[32m                        {[m
[32m+[m[32m                            Cursor phones = getContentResolver().query(ContactsContract.CommonDataKinds.Phone.CONTENT_URI, null,[m
[32m+[m[32m                                    ContactsContract.CommonDataKinds.Phone.CONTACT_ID + " = " + contactId, null, null);[m
[32m+[m[32m                            if (phones.moveToFirst())[m
[32m+[m[32m                            {[m
[32m+[m[32m                                phoneNumber = phones.getString(phones.getColumnIndex(ContactsContract.CommonDataKinds.Phone.NUMBER));[m
[32m+[m[32m                            }[m
[32m+[m[32m                            phones.close();[m
[32m+[m[32m                        }catch (Exception e){[m
[32m+[m[32m                            Log.e("error",e.getMessage());[m
[32m+[m[32m                        }[m
[32m+[m
[32m+[m
[32m+[m[32m                        EditText show = (EditText) findViewById(R.id.show);[m
[32m+[m[32m                        //显示联系人的名称[m
[32m+[m[32m                        show.setText(name);[m
[32m+[m[32m                        EditText phone = (EditText) findViewById(R.id.phone);[m
[32m+[m[32m                        phone.setText(phoneNumber);[m
[32m+[m[32m                    }[m
[32m+[m[32m                    cursor.close();[m
[32m+[m[32m                }[m
[32m+[m[32m                break;[m
[32m+[m[32m        }[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    public void home(View v){[m
[32m+[m[32m        Intent intent = new Intent();[m
[32m+[m[32m        intent.setAction(Intent.ACTION_MAIN);[m
[32m+[m[32m        intent.addCategory(Intent.CATEGORY_HOME);[m
[32m+[m[32m        startActivity(intent);[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    /**[m
[32m+[m[32m     * 同一个Intent对象使用type、data第二者会覆盖第一者value值[m
[32m+[m[32m     * @param v[m
[32m+[m[32m     */[m
[32m+[m[32m    public void dataOrtype(View v){[m
[32m+[m[32m        Intent intent = new Intent();[m
[32m+[m[32m        intent.setType("abc/xyz");[m
[32m+[m[32m        intent.setData(Uri.parse("lee://www.fkjava.org:8888/test"));[m
[32m+[m[32m        Toast.makeText(this, intent.toString(), Toast.LENGTH_SHORT).show();[m
[32m+[m[32m    }[m
[32m+[m[32m    public void dataOrtype1(View v){[m
[32m+[m[32m        Intent intent = new Intent();[m
[32m+[m[32m        intent.setData(Uri.parse("lee://www.fkjava.org:8888/test"));[m
[32m+[m[32m        intent.setType("abc/xyz");[m
[32m+[m[32m        Toast.makeText(this, intent.toString(), Toast.LENGTH_SHORT).show();[m
[32m+[m[32m    }[m
[32m+[m[32m    /**[m
[32m+[m[32m     * 同一个Intent对象使用DataAndType并存value值[m
[32m+[m[32m     *[m
[32m+[m[32m     * @param v[m
[32m+[m[32m     */[m
[32m+[m[32m    public void dataAndtype(View v){[m
[32m+[m[32m        Intent intent = new Intent();[m
[32m+[m[32m        intent.setDataAndType(Uri.parse("lee://www.fkjava.org:8888/test"),"abc/xyz");[m
[32m+[m[32m        Toast.makeText(this, intent.toString(), Toast.LENGTH_SHORT).show();[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    /**[m
[32m+[m[32m     * 使用Action、Data属性启动系统Activity[m
[32m+[m[32m     * @param v[m
[32m+[m[32m     */[m
[32m+[m[32m    public void actionAndData(View v){[m
[32m+[m[32m        Intent intent = new Intent();[m
[32m+[m[32m        String data = "http://www.crazyit.org";[m
[32m+[m[32m        //根据指定字符串解析Uri对象[m
[32m+[m[32m        Uri uri = Uri.parse(data);[m
[32m+[m[32m        //为Intent设置Action属性[m
[32m+[m[32m        intent.setAction(Intent.ACTION_VIEW);[m
[32m+[m[32m        //设置Data属性[m
[32m+[m[32m        intent.setData(uri);[m
[32m+[m[32m        startActivity(intent);[m
[32m+[m[32m    }[m
[32m+[m[32m    public void edit(View v){[m
[32m+[m[32m        try[m
[32m+[m[32m        {[m
[32m+[m[32m            Intent intent = new Intent();[m
[32m+[m[32m            String data = "content://com.android.contacts/contacts/1";[m
[32m+[m[32m            //根据指定字符串解析Uri对象[m
[32m+[m[32m            Uri uri = Uri.parse(data);[m
[32m+[m[32m            //为Intent设置Action属性（动作为：编辑）[m
[32m+[m[32m            intent.setAction(Intent.ACTION_EDIT);[m
[32m+[m[32m            //设置Data属性[m
[32m+[m[32m            intent.setData(uri);[m
[32m+[m[32m            startActivity(intent);[m
[32m+[m[32m        }catch (Exception e){[m
[32m+[m[32m            Log.e("error",e.getMessage());[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m    }[m
[32m+[m[32m    public void call(View v){[m
[32m+[m[32m        try[m
[32m+[m[32m        {[m
[32m+[m[32m            Intent intent = new Intent();[m
[32m+[m[32m            String data = "tel:136888888";[m
[32m+[m[32m            //根据指定字符串解析Uri对象[m
[32m+[m[32m            Uri uri = Uri.parse(data);[m
[32m+[m[32m            //为Intent设置Action属性（动作为：拨号）[m
[32m+[m[32m            intent.setAction(Intent.ACTION_CALL);[m
[32m+[m[32m            //设置Data属性[m
[32m+[m[32m            intent.setData(uri);[m
[32m+[m[32m            startActivity(intent);[m
[32m+[m[32m        }catch (Exception e){[m
[32m+[m[32m            Log.e("error",e.getMessage());[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m    }[m
[32m+[m[32m}[m
