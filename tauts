[32m+[m[32m        cacheCanvas = new Canvas();[m
[32m+[m[32m        path = new Path();[m
[32m+[m[32m        cacheCanvas.setBitmap(cacheBitmap);[m
[32m+[m[32m        //è®¾ç½®ç”»ç¬”çš„é¢œè‰²[m
[32m+[m[32m        paint = new Paint(Paint.DITHER_FLAG);[m
