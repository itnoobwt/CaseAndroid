[32m+[m[32mpackage com.system.caseandroid.seven.utils;[m
[32m+[m
[32m+[m[32mimport android.graphics.Bitmap;[m
[32m+[m[32mimport android.util.Log;[m
[32m+[m[32mimport android.util.LruCache;[m
[32m+[m[32mimport com.system.caseandroid.MyApplication;[m
[32m+[m
[32m+[m[32m/** 内存缓存[m
[32m+[m[32m * Created by user on 2016/11/25.[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mpublic class MemoryCacheUtils[m
[32m+[m[32m{[m
[32m+[m[32m    private LruCache<String,Bitmap> mMemorCache;[m
[32m+[m
[32m+[m[32m    public MemoryCacheUtils(){[m
[32m+[m[32m        //得到手机最大允许内存的1/8,即超过指定内存,则开始回收(MyApplication.maxMemory)[m
[32m+[m[32m        mMemorCache = new LruCache<String,Bitmap>(MyApplication.maxMemory){[m
[32m+[m[32m            @Override[m
[32m+[m[32m            protected int sizeOf(String key, Bitmap value)[m
[32m+[m[32m            {[m
[32m+[m[32m                // 重写此方法来衡量每张图片的大小，默认返回图片数量。[m
[32m+[m[32m                return value.getByteCount();[m
[32m+[m[32m            }[m
[32m+[m[32m        };[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    /**[m
[32m+[m[32m     * 添加缓存[m
[32m+[m[32m     * @param key[m
[32m+[m[32m     * @param bitmap[m
[32m+[m[32m     */[m
[32m+[m[32m    public void addBitmapToMemoryCache(String key,Bitmap bitmap){[m
[32m+[m[32m        //判断如果以后缓存就不缓存了[m
[32m+[m[32m        if(getBitmapFromMemCache(key) == null){[m
[32m+[m[32m            Log.e("size",mMemorCache.size()+"        ---------------------------   ");[m
[32m+[m[32m            mMemorCache.put(key,bitmap);[m
[32m+[m[32m            Log.e("size",mMemorCache.size()+"        ---------------------------   ");[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    /**[m
[32m+[m[32m     * 根据key获取bitmap[m
[32m+[m[32m     * @param key[m
[32m+[m[32m     * @return[m
[32m+[m[32m     */[m
[32m+[m[32m    public Bitmap getBitmapFromMemCache(String key){[m
[32m+[m[32m        Bitmap bitmap = mMemorCache.get(key);[m
[32m+[m[32m        return bitmap;[m
[32m+[m[32m    }[m
[32m+[m[32m}[m
