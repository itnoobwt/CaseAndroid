[32m+[m[32mpackage com.system.caseandroid.seven.widget;[m
[32m+[m
[32m+[m[32mimport android.content.Context;[m
[32m+[m[32mimport android.graphics.Bitmap;[m
[32m+[m[32mimport android.graphics.Canvas;[m
[32m+[m[32mimport android.graphics.Matrix;[m
[32m+[m[32mimport android.graphics.drawable.BitmapDrawable;[m
[32m+[m[32mimport android.util.AttributeSet;[m
[32m+[m[32mimport android.util.Log;[m
[32m+[m[32mimport android.view.KeyEvent;[m
[32m+[m[32mimport android.view.View;[m
[32m+[m[32mimport com.system.caseandroid.R;[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Created by user on 2016/11/18.[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mpublic class MyMatrix extends View[m
[32m+[m[32m{[m
[32m+[m[32m    private Bitmap bitmap;[m
[32m+[m[32m    private int width,height;[m
[32m+[m[32m    public boolean isScale = false;[m
[32m+[m[32m    private Matrix matrix;[m
[32m+[m[32m    public float sx = 0f;[m
[32m+[m[32m    public float scale = 1f;[m
[32m+[m
[32m+[m[32m    public MyMatrix(Context context)[m
[32m+[m[32m    {[m
[32m+[m[32m        super(context);[m
[32m+[m[32m        Log.e("MyMatrix","1111111111111111111111111111111");[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    public MyMatrix(Context context, AttributeSet attrs)[m
[32m+[m[32m    {[m
[32m+[m[32m        super(context, attrs);[m
[32m+[m[32m        Log.e("MyMatrix","22222222222222222222222222222222222");[m
[32m+[m[32m        bitmap = ((BitmapDrawable)context.getResources().getDrawable(R.mipmap.xiongmao)).getBitmap();[m
[32m+[m[32m        width = bitmap.getWidth();[m
[32m+[m[32m        height = bitmap.getHeight();[m
[32m+[m[32m        matrix = new Matrix();[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    @Override[m
[32m+[m[32m    protected void onDraw(Canvas canvas)[m
[32m+[m[32m    {[m
[32m+[m[32m        super.onDraw(canvas);[m
[32m+[m[32m        //重置Matrix[m
[32m+[m[32m        matrix.reset();[m
[32m+[m[32m        if(!isScale){[m
[32m+[m[32m            //旋转Matrix[m
[32m+[m[32m            matrix.setSkew(sx,0);[m
[32m+[m[32m        }[m
[32m+[m[32m        else{[m
[32m+[m[32m            //缩放Matrix[m
[32m+[m[32m            matrix.setScale(scale,scale);[m
[32m+[m[32m        }[m
[32m+[m[32m        //根据原始位图和Matrix创新新图[m
[32m+[m[32m        Bitmap bitmap2 = Bitmap.createBitmap(bitmap,0,0,width,height,matrix,true);[m
[32m+[m[32m        canvas.drawBitmap(bitmap2,matrix,null);[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m
[32m+[m[32m}[m
