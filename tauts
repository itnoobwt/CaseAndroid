[32m+[m[32m/**[m
[32m+[m[32m * 三级缓存[m
[32m+[m[32m */[m
[32m+[m[32mpublic class MyBitmapCache[m
[32m+[m[32m{[m
[32m+[m[32m    private NetCacheUtils mNetCacheUtils;      //网络缓存[m
[32m+[m[32m    private LocalCacheUtils mLocalCacheUtils;     //本地缓存[m
[32m+[m[32m    private MemoryCacheUtils mMemoryCacheUtils;    //内存缓存[m
[32m+[m[32m    public static MyBitmapCache myBitmapCache;[m
[32m+[m[32m    public static MyBitmapCache getInstance(){[m
[32m+[m[32m        if(myBitmapCache == null){[m
[32m+[m[32m            synchronized (MyBitmapCache.class){[m
[32m+[m[32m                myBitmapCache = new MyBitmapCache();[m
[32m+[m[32m            }[m
[32m+[m[32m        }[m
[32m+[m[32m        return  myBitmapCache;[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    public MyBitmapCache(){[m
[32m+[m[32m        mMemoryCacheUtils = new MemoryCacheUtils();[m
[32m+[m[32m        mNetCacheUtils = new NetCacheUtils();[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    /**[m
[32m+[m[32m     *  显示图片[m
[32m+[m[32m     * @param url 图片地址[m
[32m+[m[32m     * @param imageView  控件[m
[32m+[m[32m     */[m
[32m+[m[32m    public void disPlay(String url, ImageView imageView){[m
[32m+[m[32m        imageView.setImageResource(R.mipmap.icon_placeholder);[m
[32m+[m[32m        if(url == null){[m
[32m+[m[32m            Log.e("MyBitmapCache","url为null");[m
[32m+[m[32m            return;[m
[32m+[m[32m        }[m
[32m+[m[32m        if(!url.isEmpty()){[m
[32m+[m[32m            Bitmap bitmap = mMemoryCacheUtils.getBitmapFromMemCache(url);[m
[32m+[m[32m            if(bitmap!=null)[m
[32m+[m[32m                imageView.setImageBitmap(bitmap);[m
[32m+[m[32m            else{[m
[32m+[m[32m                mNetCacheUtils.getBitmapFromNetWorkCache(url,imageView);[m
[32m+[m[32m                imageView.setImageResource(R.mipmap.icon_placeholder);[m
[32m+[m[32m            }[m
[32m+[m
[32m+[m[32m        }[m
[32m+[m[32m        else{[m
[32m+[m[32m            Log.e("MyBitmapCache","url为空");[m
[32m+[m[32m        }[m
[32m+[m[32m    }[m
[32m+[m[32m}[m
