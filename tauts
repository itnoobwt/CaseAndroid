[32m+[m[32mpackage com.system.caseandroid.seven;[m
[32m+[m
[32m+[m[32mimport android.content.res.AssetManager;[m
[32m+[m[32mimport android.graphics.BitmapFactory;[m
[32m+[m[32mimport android.graphics.drawable.BitmapDrawable;[m
[32m+[m[32mimport android.os.Bundle;[m
[32m+[m[32mimport android.app.Activity;[m
[32m+[m[32mimport android.view.View;[m
[32m+[m[32mimport android.widget.ImageView;[m
[32m+[m[32mimport butterknife.BindView;[m
[32m+[m[32mimport com.system.caseandroid.BaseActivity;[m
[32m+[m[32mimport com.system.caseandroid.R;[m
[32m+[m
[32m+[m[32mimport java.io.IOException;[m
[32m+[m[32mimport java.io.InputStream;[m
[32m+[m
[32m+[m
[32m+[m[32mpublic class ActivityBitmapAssetsActivity extends BaseActivity[m
[32m+[m[32m{[m
[32m+[m[32m    String[] images = null;[m
[32m+[m[32m    AssetManager assets = null;[m
[32m+[m[32m    int currentImg = 0;[m
[32m+[m[32m    @BindView(R.id.image_view)[m
[32m+[m[32m    ImageView imageview;[m
[32m+[m[32m    @Override[m
[32m+[m[32m    protected void onCreate(Bundle savedInstanceState) {[m
[32m+[m[32m        super.onCreate(savedInstanceState);[m
[32m+[m[32m        setContentView(R.layout.activity_bitmap_assets);[m
[32m+[m[32m        assets = getAssets();[m
[32m+[m[32m        try[m
[32m+[m[32m        {[m
[32m+[m[32m            //在assets/img文件下找图片[m
[32m+[m[32m            images = assets.list("img");[m
[32m+[m
[32m+[m[32m        }[m
[32m+[m[32m        catch (IOException e)[m
[32m+[m[32m        {[m
[32m+[m[32m            e.printStackTrace();[m
[32m+[m[32m        }[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    public void btnBitmap(View view){[m
[32m+[m[32m        if(currentImg >= images.length){[m
[32m+[m[32m            currentImg = 0;[m
[32m+[m[32m        }[m
[32m+[m[32m        while (!images[currentImg].endsWith(".png")&&!images[currentImg].endsWith(".jpg")&&!images[currentImg][m
[32m+[m[32m                .endsWith(".gif")){[m
[32m+[m[32m            currentImg++;[m
[32m+[m[32m            if(currentImg >= images.length){[m
[32m+[m[32m                currentImg = 0;[m
[32m+[m[32m            }[m
[32m+[m[32m        }[m
[32m+[m[32m        InputStream assetFile = null;[m
[32m+[m[32m        try[m
[32m+[m[32m        {[m
[32m+[m[32m            assetFile = assets.open("img/"+images[currentImg++]);[m
[32m+[m[32m        }[m
[32m+[m[32m        catch (IOException e)[m
[32m+[m[32m        {[m
[32m+[m[32m            e.printStackTrace();[m
[32m+[m[32m        }[m
[32m+[m[32m        BitmapDrawable bitmapDrawable = (BitmapDrawable) imageview.getDrawable();[m
[32m+[m[32m        // 如果图片还未回收，先强制回收该图片[m
[32m+[m[32m        if(bitmapDrawable!= null && !bitmapDrawable.getBitmap().isRecycled()){[m
[32m+[m[32m            bitmapDrawable.getBitmap().recycle();[m
[32m+[m[32m        }[m
[32m+[m[32m        // 改变ImageView显示的图片[m
[32m+[m[32m        imageview.setImageBitmap(BitmapFactory.decodeStream(assetFile));[m
[32m+[m[32m    }[m
[32m+[m[32m}[m
