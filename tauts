[32m+[m[32m                my_matrix.isScale = true;[m
[32m+[m[32m                if(my_matrix.scale > 0.5 )[m
[32m+[m[32m                    my_matrix.scale -= 0.1;[m
[32m+[m[32m                my_matrix.postInvalidate();[m
[32m+[m[32m                break;[m
[32m+[m[32m        }[m
[32m+[m[32m        return super.onKeyDown(keyCode, event);[m
[32m+[m[32m    }[m
