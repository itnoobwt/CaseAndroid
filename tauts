[32m+[m[32mpackage com.system.caseandroid.seven.widget;[m
[32m+[m
[32m+[m[32mimport android.content.Context;[m
[32m+[m[32mimport android.graphics.*;[m
[32m+[m[32mimport android.util.AttributeSet;[m
[32m+[m[32mimport android.view.View;[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Created by user on 2016/10/14.[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mpublic class MyView extends View[m
[32m+[m[32m{[m
[32m+[m[32m    public MyView(Context context, AttributeSet attrs)[m
[32m+[m[32m    {[m
[32m+[m[32m        super(context, attrs);[m
[32m+[m[32m    }[m
[32m+[m[32m    @Override[m
[32m+[m[32m    protected void onDraw(Canvas canvas)[m
[32m+[m[32m    {[m
[32m+[m[32m        super.onDraw(canvas);[m
[32m+[m[32m        canvas.drawColor(Color.WHITE);[m
[32m+[m[32m        Paint paint = new Paint();[m
[32m+[m[32m        paint.setAntiAlias(true);[m
[32m+[m[32m        paint.setStrokeWidth(4);[m
[32m+[m[32m        paint.setStyle(Paint.Style.STROKE);[m
[32m+[m[32m        paint.setColor(Color.BLUE);[m
[32m+[m[32m        paint.setShadowLayer(25,20,20,Color.GRAY);[m
[32m+[m[32m        int viewWidth = this.getWidth();[m
[32m+[m[32m        canvas.drawCircle(viewWidth/10+10,viewWidth/10+10,viewWidth/10,paint);  //圆形[m
[32m+[m[32m        canvas.drawRect(10,viewWidth/5+20,viewWidth/5+10,viewWidth*2/5+20,paint);//正方形[m
[32m+[m[32m        canvas.drawRoundRect(10,viewWidth/2+40,10+viewWidth/5,viewWidth*3/5+40,15,15,paint); //绘制圆角矩形[m
[32m+[m[32m        RectF re11 = new RectF(10, viewWidth * 3 / 5 + 50[m
[32m+[m[32m                , 10 + viewWidth / 5 ,viewWidth * 7 / 10 + 50);[m
[32m+[m[32m        canvas.drawOval(re11,paint); //椭圆[m
[32m+[m
[32m+[m[32m        Path path = new Path();[m
[32m+[m[32m        path.moveTo(10,viewWidth*9/10+60);[m
[32m+[m[32m        path.lineTo(viewWidth/5+10,viewWidth*9/10+60);[m
[32m+[m[32m        path.lineTo(viewWidth/10+10,viewWidth*7/10+60);[m
[32m+[m[32m        path.close();[m
[32m+[m[32m        canvas.drawPath(path,paint); //绘制三角形[m
[32m+[m[32m    }[m
[32m+[m[32m}[m
