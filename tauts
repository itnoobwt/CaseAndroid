[32m+[m[32mpublic class PathActivity extends BaseActivity[m
[32m+[m[32m{[m
[32m+[m
[32m+[m[32m    @Override[m
[32m+[m[32m    protected void onCreate(Bundle savedInstanceState)[m
[32m+[m[32m    {[m
[32m+[m[32m        super.onCreate(savedInstanceState);[m
[32m+[m[32m        setContentView(new MyView(PathActivity.this));[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    class MyView extends View{[m
[32m+[m
[32m+[m[32m        private float phase;[m
[32m+[m[32m        PathEffect[] effects = new PathEffect[7];[m
[32m+[m[32m        int[] colors;[m
[32m+[m[32m        private Paint paint;[m
[32m+[m[32m        private Path path;[m
[32m+[m[32m        public MyView(Context context )[m
[32m+[m[32m        {[m
[32m+[m[32m            super(context);[m
[32m+[m[32m            paint = new Paint();[m
[32m+[m[32m            paint.setStyle(Paint.Style.STROKE);[m
[32m+[m[32m            paint.setStrokeWidth(4);[m
[32m+[m[32m            //åˆ›å»ºå¹¶åˆå§‹åŒ–Path[m
[32m+[m[32m            path = new Path();[m
[32m+[m[32m            path.moveTo(0,0);[m
[32m+[m[32m            for (int i = 1; i<=40; i++){[m
[32m+[m[32m                path.lineTo(i*20,(float)Math.random()*60);[m
[32m+[m[32m            }[m
[32m+[m[32m            //åˆå§‹åŒ–7é¢œè‰²[m
[32m+[m[32m            colors = new int[]{Color.BLACK,Color.BLUE,Color.CYAN,Color.GREEN[m
[32m+[m[32m            ,Color.MAGENTA,Color.RED,Color.YELLOW};[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m
[32m+[m[32m        @Override[m
[32m+[m[32m        protected void onDraw(Canvas canvas)[m
[32m+[m[32m        {[m
[32m+[m[32m            super.onDraw(canvas);[m
[32m+[m[32m            //å°†åŒ—äº¬å¡«å……ä¸ºç™½è‰²[m
[32m+[m[32m            canvas.drawColor(Color.LTGRAY);[m
[32m+[m[32m            //------------- ä¸‹é¢å¼€å§‹åˆå§‹åŒ–7ä¸­è·¯å¾„æ•ˆæœ   -----------------[m
[32m+[m[32m            //ä¸é€‚ç”¨è·¯å¾„æ•ˆæœ[m
[32m+[m[32m            effects[0] = null;[m
[32m+[m[32m            //ä½¿ç”¨CornerPathEffectè·¯å¾„æ•ˆæœ[m
[32m+[m[32m            effects[1] = new CornerPathEffect(10);[m
[32m+[m[32m            //åˆå§‹åŒ–DiscretePathEffect[m
[32m+[m[32m            effects[2] = new DiscretePathEffect(3.0f,5.0f);[m
[32m+[m[32m            //åˆå§‹åŒ–DashPathEffect[m
[32m+[m[32m            effects[3] = new DashPathEffect(new float[]{20,10,5,10},phase);[m
[32m+[m[32m            //åˆå§‹åŒ–PathDashPathEffect[m
[32m+[m[32m            Path p = new Path();[m
[32m+[m[32m            p.addRect(0,0,8,8, Path.Direction.CCW);[m
[32m+[m[32m            effects[4] = new PathDashPathEffect(p,12,phase,PathDashPathEffect.Style.ROTATE);[m
[32m+[m[32m            //åˆå§‹åŒ–ComposePathEffect[m
[32m+[m[32m            effects[5] = new ComposePathEffect(effects[2],effects[4]);[m
[32m+[m[32m            effects[6] = new SumPathEffect(effects[4],effects[3]);[m
[32m+[m[32m            //å°†ç”»å¸ƒç§»åŠ¨åˆ°ï¼ˆ8ï¼Œ8ï¼‰å¤„å¼€å§‹ç»˜åˆ¶[m
[32m+[m[32m            canvas.translate(8,8);[m
[32m+[m[32m            //ä¾æ¬¡ä½¿ç”¨7ç§ä¸åŒçš„è·¯å¾„æ•ˆæœã€7ç§ä¸åŒçš„é¢œè‰²æ¥ç»˜åˆ¶è·¯å¾„[m
[32m+[m[32m            for (int i = 0; i< effects.length; i++){[m
[32m+[m[32m                paint.setPathEffect(effects[i]);[m
[32m+[m[32m                paint.setColor(colors[i]);[m
[32m+[m[32m                canvas.drawPath(path,paint);[m
[32m+[m[32m                canvas.translate(0,60);[m
[32m+[m[32m            }[m
[32m+[m[32m            //æ”¹å˜phaseå€¼ï¼Œå½¢æˆåŠ¨ç”»æ•ˆæœ[m
[32m+[m[32m            phase += 1;[m
[32m+[m[32m            invalidate();[m
[32m+[m[32m        }[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m
[32m+[m[32m}[m
