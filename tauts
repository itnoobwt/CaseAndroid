[32m+[m[32m        {[m
[32m+[m[32m            @Override[m
[32m+[m[32m            public boolean onKey(View v, int keyCode, KeyEvent event)[m
[32m+[m[32m            {[m
[32m+[m[32m                switch (event.getKeyCode()){[m
[32m+[m[32m                    case KeyEvent.KEYCODE_A:[m
