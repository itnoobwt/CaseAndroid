[32m+[m[32m                    handler.sendMessage(msg);[m
[32m+[m
[32m+[m[32m                    if(drawable.getLevel() >=10000)[m
[32m+[m[32m                        timer.cancel();[m
[32m+[m[32m                }catch (Exception e){[m
