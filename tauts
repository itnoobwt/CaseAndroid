[32m+[m[32mpublic class LanguageActivity extends BaseActivity[m
[32m+[m[32m{[m
[32m+[m[32m    private TextView txt;[m
[32m+[m[32m    @Override[m
[32m+[m[32m    protected void onCreate(Bundle savedInstanceState)[m
[32m+[m[32m    {[m
[32m+[m[32m        super.onCreate(savedInstanceState);[m
[32m+[m[32m        setContentView(R.layout.activity_language);[m
[32m+[m[32m        txt = (TextView) findViewById(R.id.language_txt);[m
[32m+[m[32m        language();[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    public  void language(){[m
[32m+[m[32m        Locale[] locales = Locale.getAvailableLocales();[m
[32m+[m[32m        StringBuilder str = new StringBuilder();[m
[32m+[m[32m        for ( int i = 0; i<locales.length; i++){[m
[32m+[m[32m            str.append(locales[i].getDisplayCountry()+" = "+ locales[i].getCountry()+[m
[32m+[m[32m            "  "+locales[i].getDisplayLanguage()+"  = "+locales[i].getLanguage()+"\n");[m
[32m+[m[32m        }[m
[32m+[m[32m        txt.setText(str.toString());[m
[32m+[m[32m    }[m
[32m+[m[32m}[m
