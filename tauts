[32m+[m[32mpackage com.system.caseandroid.seven.widget;[m
[32m+[m
[32m+[m[32mimport android.content.Context;[m
[32m+[m[32mimport android.graphics.Canvas;[m
[32m+[m[32mimport android.graphics.Color;[m
[32m+[m[32mimport android.graphics.Paint;[m
[32m+[m[32mimport android.util.DisplayMetrics;[m
[32m+[m[32mimport android.view.Display;[m
[32m+[m[32mimport android.view.View;[m
[32m+[m
[32m+[m[32mimport java.util.Random;[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Created by user on 2016/10/20.[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mpublic class GameView extends View[m
[32m+[m[32m{[m
[32m+[m[32m    public Paint paint;[m
[32m+[m[32m    // 游戏是否结束的旗标[m
[32m+[m[32m    private boolean isLose = false;[m
[32m+[m[32m    public DisplayMetrics displayMetrics;[m
[32m+[m[32m    //球拍的垂直位置[m
[32m+[m[32m    public int racketY;[m
[32m+[m[32m    //定义球拍的高度和宽度[m
[32m+[m[32m    private final int RACKET_HEIGHT = 30;[m
[32m+[m[32m    public final int RACKET_WIDTH = 30;[m
[32m+[m
[32m+[m[32m    //小球的大小[m
[32m+[m[32m    private final int BALL_SIZE = 16;[m
[32m+[m
[32m+[m[32m    //小球纵向的运行速度[m
[32m+[m[32m    private int ySpeed = 15;[m
[32m+[m
[32m+[m[32m    Random rand = new Random();[m
[32m+[m[32m    private int ballX = rand.nextInt(200) + 20;[m
[32m+[m[32m    private int ballY = rand.nextInt(10) + 10;[m
[32m+[m[32m    public int racketX = rand.nextInt(200);[m
[32m+[m
[32m+[m[32m    public GameView(Context context,Display display)[m
[32m+[m[32m    {[m
[32m+[m[32m        super(context);[m
[32m+[m[32m        paint = new Paint();[m
[32m+[m[32m        paint.setStyle(Paint.Style.FILL);[m
[32m+[m[32m        paint.setAntiAlias(true);[m
[32m+[m[32m        displayMetrics = new DisplayMetrics();[m
[32m+[m[32m        display.getMetrics(displayMetrics);[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    @Override[m
[32m+[m[32m    protected void onDraw(Canvas canvas)[m
[32m+[m[32m    {[m
[32m+[m[32m        super.onDraw(canvas);[m
[32m+[m[32m        if(isLose){[m
[32m+[m[32m            paint.setColor(Color.RED);[m
[32m+[m[32m            paint.setTextSize(40);[m
[32m+[m[32m            canvas.drawText("游戏已结束",displayMetrics.widthPixels/2-100,200,paint);[m
[32m+[m[32m        }else{[m
[32m+[m[32m            //设置颜色，并绘制小球[m
[32m+[m[32m            paint.setColor(Color.rgb(255,0,0));[m
[32m+[m[32m            canvas.drawCircle(ballX,ballY,BALL_SIZE,paint);[m
[32m+[m
[32m+[m[32m            //设置颜色，并绘制球拍[m
[32m+[m[32m            paint.setColor(Color.rgb(80,80,200));[m
[32m+[m[32m            canvas.drawRect(racketX,racketY,racketX+RACKET_WIDTH,racketY+RACKET_HEIGHT,paint);[m
[32m+[m[32m        }[m
[32m+[m[32m    }[m
[32m+[m[32m}[m
