[32m+[m[32m    @Override[m
[32m+[m[32m    public boolean onKeyDown(int keyCode, KeyEvent event)[m
[32m+[m[32m    {[m
[32m+[m[32m        switch (keyCode){[m
[32m+[m[32m            case KeyEvent.KEYCODE_4:[m
[32m+[m[32m                my_matrix.isScale = false;[m
[32m+[m[32m                my_matrix.sx += 0.1;[m
[32m+[m[32m                my_matrix.postInvalidate();[m
[32m+[m[32m                break;[m
[32m+[m[32m            case KeyEvent.KEYCODE_6:[m
[32m+[m[32m                my_matrix.isScale = false;[m
[32m+[m[32m                my_matrix.sx -= 0.1;[m
[32m+[m[32m                my_matrix.postInvalidate();[m
[32m+[m[32m                break;[m
[32m+[m[32m            case KeyEvent.KEYCODE_8:[m
[32m+[m[32m                my_matrix.isScale = true;[m
[32m+[m[32m                if(my_matrix.scale < 2 )[m
[32m+[m[32m                    my_matrix.scale += 0.1;[m
[32m+[m[32m                my_matrix.postInvalidate();[m
[32m+[m[32m                break;[m
[32m+[m[32m            case KeyEvent.KEYCODE_2:[m
[32m+[m[32m                my_matrix.isScale = true;[m
[32m+[m[32m                if(my_matrix.scale > 0.5 )[m
[32m+[m[32m                    my_matrix.scale -= 0.1;[m
[32m+[m[32m                my_matrix.postInvalidate();[m
[32m+[m[32m                break;[m
[32m+[m[32m        }[m
[32m+[m[32m        return super.onKeyDown(keyCode, event);[m
[32m+[m[32m    }[m
[32m+[m[32m}[m
