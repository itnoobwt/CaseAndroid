[32m+[m[32m    protected void onCreate(Bundle savedInstanceState)[m
[32m+[m[32m    {[m
[32m+[m[32m        super.onCreate(savedInstanceState);[m
[32m+[m[32m        setContentView(R.layout.activity_native);[m
[32m+[m[32m        ed = (EditText) findViewById(R.id.xml_ed);[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    public void getXmlValue(View v){[m
[32m+[m[32m        //æ ¹æ®XMLèµ„æºçš„IDè·å–è§£æè¯¥èµ„æºçš„è§£æå™¨[m
[32m+[m[32m        XmlResourceParser xrp = getResources().getXml(R.xml.books);[m
[32m+[m[32m        StringBuilder stringBuilder = new StringBuilder();[m
[32m+[m[32m        try[m
[32m+[m[32m        {[m
[32m+[m[32m            while (xrp.getEventType() != XmlPullParser.END_DOCUMENT){[m
[32m+[m[32m                if(xrp.getEventType() == XmlPullParser.START_TAG){[m
[32m+[m[32m                    String tagName = xrp.getName();[m
[32m+[m[32m                    if(tagName.equals("book")){[m
[32m+[m[32m                        //æ ¹æ®å±æ€§åæ¥è·å–å±æ€§å€¼[m
[32m+[m[32m                        String bookName = xrp.getAttributeValue(null,"price");[m
[32m+[m[32m                        stringBuilder.append("ä»·æ ¼ï¼š");[m
[32m+[m[32m                        stringBuilder.append(bookName);[m
[32m+[m[32m                        //æ ¹æ®å±æ€§ç´¢å¼•æ¥è·å–å±æ€§å€¼[m
[32m+[m[32m                        String bookPrice = xrp.getAttributeValue(1);[m
[32m+[m[32m                        stringBuilder.append("   å‡ºç‰ˆæ—¥æœŸï¼š"+bookPrice);[m
[32m+[m[32m                        stringBuilder.append(xrp.nextText());[m
[32m+[m[32m                    }[m
[32m+[m[32m                    stringBuilder.append("\n");[m
[32m+[m[32m                }[m
[32m+[m[32m                //è·å–è§£æå™¨çš„ä¸‹ä¸€ä¸ªäº‹ä»¶[m
[32m+[m[32m                xrp.next();[m
[32m+[m[32m            }[m
[32m+[m[32m            ed.setText(stringBuilder.toString());[m
[32m+[m[32m        }[m
[32m+[m[32m        catch (XmlPullParserException e)[m
[32m+[m[32m        {[m
[32m+[m[32m            e.printStackTrace();[m
[32m+[m[32m        }[m
[32m+[m[32m        catch (IOException e)[m
[32m+[m[32m        {[m
[32m+[m[32m            e.printStackTrace();[m
[32m+[m[32m        }[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m}[m
