[32m+[m[32m                            gameView.racketX -=10;[m
[32m+[m[32m                        break;[m
[32m+[m[32m                    case KeyEvent.KEYCODE_D:[m
[32m+[m[32m                        if(gameView.racketX < gameView.displayMetrics.widthPixels - gameView.RACKET_WIDTH)[m
[32m+[m[32m                            gameView.racketX += 10;[m
[32m+[m[32m                        break;[m
[32m+[m[32m                }[m
[32m+[m[32m                gameView.invalidate();[m
[32m+[m[32m                return true;[m
[32m+[m[32m            }[m
[32m+[m[32m        });[m
[32m+[m
[32m+[m[32m        gameView.setOnTouchListener(new View.OnTouchListener()[m
[32m+[m[32m        {[m
