[32m+[m[32mpublic class NetCacheUtils[m
[32m+[m[32m{[m
[32m+[m[32m    private static final String TAG = "NetCacheUtils";[m
[32m+[m[32m    private LocalCacheUtils mLocalCacheUtils;     //æœ¬åœ°ç¼“å­˜[m
[32m+[m[32m    private MemoryCacheUtils mMemoryCacheUtils;    //å†…å­˜ç¼“å­˜[m
[32m+[m[32m    public NetCacheUtils(){[m
[32m+[m[32m        mMemoryCacheUtils = new MemoryCacheUtils();[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    /**[m
[32m+[m[32m     * ç½‘ç»œè·å–å›¾ç‰‡ï¼Œå¹¶ä¸”æ·»åŠ åˆ°ç¼“å­˜[m
[32m+[m[32m     */[m
[32m+[m[32m    public void getBitmapFromNetWorkCache(String url, ImageView imageView){[m
[32m+[m[32m        BitmapTask bitmapTask = new BitmapTask();[m
[32m+[m[32m        bitmapTask.execute(url,imageView);[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m
[32m+[m[32m    class BitmapTask extends AsyncTask<Object,Void,Bitmap>{[m
[32m+[m[32m        ImageView imageView;[m
[32m+[m[32m        String url;[m
[32m+[m[32m        @Override[m
[32m+[m[32m        protected void onPreExecute()[m
[32m+[m[32m        {[m
[32m+[m[32m            super.onPreExecute();[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        @Override[m
[32m+[m[32m        protected Bitmap doInBackground(Object... params)[m
[32m+[m[32m        {[m
[32m+[m[32m            url = (String) params[0];[m
[32m+[m[32m            imageView = (ImageView) params[1];[m
[32m+[m[32m            return getNetWork(url);[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        @Override[m
[32m+[m[32m        protected void onPostExecute(Bitmap bitmap)[m
[32m+[m[32m        {[m
[32m+[m[32m            super.onPostExecute(bitmap);[m
[32m+[m[32m            if(bitmap!=null){[m
[32m+[m[32m                imageView.setImageBitmap(bitmap);[m
[32m+[m[32m                mMemoryCacheUtils.addBitmapToMemoryCache(url,bitmap);[m
[32m+[m[32m            }[m
[32m+[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    /**[m
[32m+[m[32m     * ç½‘ç»œè¯·æ±‚å›¾ç‰‡åœ°å€[m
[32m+[m[32m     * @param path[m
[32m+[m[32m     * @return[m
[32m+[m[32m     */[m
[32m+[m[32m    public Bitmap getNetWork(String path){[m
[32m+[m[32m        try[m
[32m+[m[32m        {[m
[32m+[m[32m            URL url = new URL(path);[m
[32m+[m[32m            URLConnection connection = url.openConnection();[m
[32m+[m[32m            connection.setReadTimeout(30000);[m
[32m+[m[32m            connection.setConnectTimeout(30000);[m
[32m+[m[32m            connection.connect();[m
[32m+[m[32m            InputStream inputStream = connection.getInputStream();[m
[32m+[m[32m            return BitmapFactory.decodeStream(inputStream);[m
[32m+[m[32m        }[m
[32m+[m[32m        catch (MalformedURLException e)[m
[32m+[m[32m        {[m
[32m+[m[32m            e.printStackTrace();[m
[32m+[m[32m            Log.e(TAG,e.getMessage()+" ");[m
[32m+[m[32m        }[m
[32m+[m[32m        catch (IOException e)[m
[32m+[m[32m        {[m
[32m+[m[32m            e.printStackTrace();[m
[32m+[m[32m            Log.e(TAG,e.getMessage()+" ");[m
[32m+[m[32m        }[m
[32m+[m[32m        return null;[m
[32m+[m[32m    }[m
[32m+[m[32m}[m
