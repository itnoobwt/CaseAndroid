[32m+[m[32m        //获取该应用的AssetManger[m
[32m+[m[32m        AssetManager assets = getAssets();[m
[32m+[m[32m        try[m
[32m+[m[32m        {[m
[32m+[m[32m            AssetFileDescriptor afd = assets.openFd("shot.mp3");[m
[32m+[m[32m            player1 = new MediaPlayer();[m
[32m+[m[32m            //使用MediaPlayer加载指定的声音文件[m
[32m+[m[32m            player1.setDataSource(afd.getFileDescriptor());[m
[32m+[m[32m            player1.prepare();[m
[32m+[m[32m        }catch (IOException ex){[m
[32m+[m[32m            ex.printStackTrace();[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        playAsset.setOnClickListener(new View.OnClickListener()[m
[32m+[m[32m        {[m
[32m+[m[32m            @Override[m
[32m+[m[32m            public void onClick(View v)[m
[32m+[m[32m            {[m
[32m+[m[32m                player1.start();[m
[32m+[m[32m            }[m
[32m+[m[32m        });[m
[32m+[m
[32m+[m
[32m+[m[32m        Button playRaw = (Button) findViewById(R.id.playRaw);[m
[32m+[m[32m        playRaw.setOnClickListener(new View.OnClickListener()[m
[32m+[m[32m        {[m
[32m+[m[32m            @Override[m
[32m+[m[32m            public void onClick(View v)[m
[32m+[m[32m            {[m
