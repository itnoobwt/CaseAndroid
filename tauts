[32m+[m[32m/**[m
[32m+[m[32m * 弹球小游戏[m
[32m+[m[32m */[m
[32m+[m[32mpublic class BallActivity extends BaseActivity[m
[32m+[m[32m{[m
[32m+[m[32m    GameView gameView;[m
[32m+[m[32m    @Override[m
[32m+[m[32m    protected void onCreate(Bundle savedInstanceState)[m
[32m+[m[32m    {[m
[32m+[m[32m        super.onCreate(savedInstanceState);[m
[32m+[m[32m        requestWindowFeature(Window.FEATURE_NO_TITLE);[m
[32m+[m[32m        Display display = getWindowManager().getDefaultDisplay();[m
[32m+[m[32m        gameView = new GameView(this,display);[m
[32m+[m[32m        setContentView(gameView);[m
[32m+[m[32m        gameView.setOnKeyListener(new View.OnKeyListener()[m
[32m+[m[32m        {[m
[32m+[m[32m            @Override[m
[32m+[m[32m            public boolean onKey(View v, int keyCode, KeyEvent event)[m
[32m+[m[32m            {[m
[32m+[m[32m                switch (event.getKeyCode()){[m
[32m+[m[32m                    case KeyEvent.KEYCODE_A:[m
[32m+[m[32m                        if(gameView.racketX > 0)[m
[32m+[m[32m                            gameView.racketX -=10;[m
[32m+[m[32m                        break;[m
[32m+[m[32m                    case KeyEvent.KEYCODE_D:[m
[32m+[m[32m                        if(gameView.racketX < gameView.displayMetrics.widthPixels - gameView.RACKET_WIDTH)[m
[32m+[m[32m                            gameView.racketX += 10;[m
[32m+[m[32m                        break;[m
[32m+[m[32m                }[m
[32m+[m[32m                gameView.invalidate();[m
[32m+[m[32m                return true;[m
[32m+[m[32m            }[m
[32m+[m[32m        });[m
[32m+[m
[32m+[m[32m        gameView.setOnTouchListener(new View.OnTouchListener()[m
[32m+[m[32m        {[m
[32m+[m[32m            @Override[m
[32m+[m[32m            public boolean onTouch(View v, MotionEvent event)[m
[32m+[m[32m            {[m
[32m+[m[32m                switch (event.getAction()){[m
[32m+[m[32m                    case MotionEvent.ACTION_DOWN:[m
[32m+[m[32m                        if(gameView.racketX > 0)[m
[32m+[m[32m                            gameView.racketX -=10;[m
[32m+[m[32m                        else[m
[32m+[m[32m                            gameView.racketX +=10;[m
[32m+[m[32m                        break;[m
[32m+[m[32m                    case MotionEvent.ACTION_MOVE:[m
[32m+[m[32m                        if(gameView.racketX > 0)[m
[32m+[m[32m                            gameView.racketX -=10;[m
[32m+[m[32m                        else[m
[32m+[m[32m                            gameView.racketX +=10;[m
[32m+[m[32m                        break;[m
[32m+[m[32m                    case MotionEvent.ACTION_UP:[m
[32m+[m[32m                        if(gameView.racketX > 0)[m
[32m+[m[32m                            gameView.racketX -=10;[m
[32m+[m[32m                        else[m
[32m+[m[32m                            gameView.racketX +=10;[m
[32m+[m[32m                        break;[m
[32m+[m[32m                }[m
[32m+[m[32m                gameView.invalidate();[m
[32m+[m[32m                return true;[m
[32m+[m[32m            }[m
[32m+[m[32m        });[m
[32m+[m[32m    }[m
[32m+[m[32m    Handler handler = new Handler(){[m
[32m+[m[32m        @Override[m
[32m+[m[32m        public void handleMessage(Message msg)[m
[32m+[m[32m        {[m
[32m+[m[32m            if(msg.what == 0x123)[m
[32m+[m[32m                gameView.invalidate();[m
[32m+[m[32m        }[m
[32m+[m[32m    };[m
[32m+[m
[32m+[m[32m}[m
