[32m+[m[32m    /**[m
[32m+[m[32m     * 获取到图片的大小[m
[32m+[m[32m     */[m
[32m+[m[32m    @OnClick(R.id.btn_img_size)[m
[32m+[m[32m    public void getCount()[m
[32m+[m[32m    {[m
[32m+[m[32m        int count = BitmapFactory.decodeResource(getResources(), R.mipmap.ic_launcher).getByteCount();[m
[32m+[m[32m        Bitmap bitmap = BitmapFactory.decodeResource(getResources(), R.mipmap.xiongmao);[m
