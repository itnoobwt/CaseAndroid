[32m+[m[32mpackage com.system.caseandroid.seven.widget;[m
[32m+[m
[32m+[m[32mimport android.content.Context;[m
[32m+[m[32mimport android.graphics.*;[m
[32m+[m[32mimport android.view.MotionEvent;[m
[32m+[m[32mimport android.view.View;[m
[32m+[m
[32m+[m[32m/**绘制，双缓冲技术[m
[32m+[m[32m * Created by user on 2016/10/17.[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mpublic class DrawView extends View[m
[32m+[m[32m{[m
[32m+[m[32m    float preX;[m
[32m+[m[32m    float preY;[m
[32m+[m[32m    private Path path;[m
[32m+[m[32m    public Paint paint;[m
[32m+[m[32m    //定义内存中的一个图片，该图片将作为缓冲区[m
[32m+[m[32m    Bitmap cacheBitmap = null;[m
[32m+[m[32m    //定义cacheBitmap上的Canvas对象[m
[32m+[m[32m    private Canvas cacheCanvas = null;[m
[32m+[m[32m    private Canvas c;[m
[32m+[m[32m    public DrawView(Context context, int width,int height)[m
[32m+[m[32m    {[m
[32m+[m[32m        super(context);[m
[32m+[m[32m        //创建一个该View具有项目大小的缓冲区[m
[32m+[m[32m        cacheBitmap = Bitmap.createBitmap(width,height, Bitmap.Config.ARGB_8888);[m
[32m+[m[32m        cacheCanvas = new Canvas();[m
[32m+[m[32m        path = new Path();[m
[32m+[m[32m        cacheCanvas.setBitmap(cacheBitmap);[m
[32m+[m[32m        //设置画笔的颜色[m
[32m+[m[32m        paint = new Paint(Paint.DITHER_FLAG);[m
[32m+[m[32m        paint.setColor(Color.RED);[m
[32m+[m[32m        // 设置画笔风格[m
[32m+[m[32m        paint.setStyle(Paint.Style.STROKE);[m
[32m+[m[32m        paint.setStrokeWidth(1);[m
[32m+[m[32m        //反锯齿[m
[32m+[m[32m        paint.setAntiAlias(true);[m
[32m+[m[32m        paint.setDither(true);[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    @Override[m
[32m+[m[32m    public boolean onTouchEvent(MotionEvent event)[m
[32m+[m[32m    {[m
[32m+[m[32m        float x = event.getX();[m
[32m+[m[32m        float y = event.getY();[m
[32m+[m[32m        switch (event.getAction()){[m
[32m+[m[32m            case MotionEvent.ACTION_DOWN:[m
[32m+[m[32m                path.moveTo(x,y);[m
[32m+[m[32m                preX = x;[m
[32m+[m[32m                preY = y;[m
[32m+[m[32m                break;[m
[32m+[m[32m            case MotionEvent.ACTION_MOVE:[m
[32m+[m[32m                path.quadTo(preX,preY,x,y);[m
[32m+[m[32m                preX = x;[m
[32m+[m[32m                preY = y;[m
[32m+[m[32m                break;[m
[32m+[m[32m            case MotionEvent.ACTION_UP:[m
[32m+[m[32m                //讲cacheBitmap绘制到该View组件上[m
[32m+[m[32m                cacheCanvas.drawPath(path,paint);[m
[32m+[m[32m                path.reset();  //清除掉path里的线条和曲线[m
[32m+[m[32m                break;[m
[32m+[m[32m        }[m
[32m+[m[32m        invalidate();[m
[32m+[m[32m        return true;[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    @Override[m
[32m+[m[32m    protected void onDraw(Canvas canvas)[m
[32m+[m[32m    {[m
[32m+[m[32m        super.onDraw(canvas);[m
[32m+[m[32m        this.c = canvas;[m
[32m+[m[32m        Paint bmpPaint = new Paint();[m
[32m+[m[32m        canvas.drawBitmap(cacheBitmap,0,0,bmpPaint);[m
[32m+[m[32m        canvas.drawPath(path,paint);[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    public void clearCanvas(int width,int height){[m
[32m+[m[32m        c.restore();[m
